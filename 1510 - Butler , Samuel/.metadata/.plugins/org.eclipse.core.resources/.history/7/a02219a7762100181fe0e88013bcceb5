import java.util.*;
public class test5
{
	public static void main(String[] args)
	{
		A a = new A();
		a.p(10);
		a.p(10.0);
	}
}
class B
{
	public void p(double i)
	{
		System.out.println(i * 2);
		System.out.println(2);
	}
}
class A extends B
{
	public void p(int i)
	{
		System.out.println(i);
		System.out.println(1);
	}
}